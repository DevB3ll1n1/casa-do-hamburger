generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    String @id @default(uuid())
  name  String
  email String @unique
  password String
  cep String
  admin Boolean @default(false)
}

model Product {
  id String @id @default(uuid())
  name String
  description String
  price Float
  category String 
  imageUrl String
  createAt DateTime @default(now())
}


